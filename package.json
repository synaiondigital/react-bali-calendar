{
    "name": "react-bali-calendar",
    "version": "1.2.4",
    "description": "React datepicker for Balinese Calendar with Indonesian National Holidays",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "default": "./dist/index.mjs"
        },
        "./utils": {
            "types": "./dist/utils.d.ts",
            "import": "./dist/utils.mjs",
            "require": "./dist/utils.js",
            "default": "./dist/utils.mjs"
        },
        "./hooks": {
            "types": "./dist/hooks.d.ts",
            "import": "./dist/hooks.mjs",
            "require": "./dist/hooks.js",
            "default": "./dist/hooks.mjs"
        },
        "./styles.css": "./dist/index.css"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "demo": "vite demo",
        "test": "vitest run",
        "test:watch": "vitest",
        "lint": "eslint src/**/*.{ts,tsx}",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run build"
    },
    "peerDependencies": {
        "react": ">=17",
        "react-dom": ">=17"
    },
    "dependencies": {
        "balinese-date-js-lib": "^0.4.3",
        "date-fns": "^3.0.0"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@vitejs/plugin-react": "^5.1.2",
        "autoprefixer": "^10.4.17",
        "postcss": "^8.4.35",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "tailwindcss": "^3.4.1",
        "tsup": "^8.0.2",
        "typescript": "^5.3.3",
        "vite": "^7.3.0",
        "vitest": "^1.3.1"
    },
    "keywords": [
        "react",
        "calendar",
        "datepicker",
        "bali",
        "balinese",
        "pawukon",
        "saka",
        "indonesia",
        "holiday",
        "galungan",
        "kuningan",
        "nyepi"
    ],
    "author": "synaiondigital",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/synaiondigital/react-bali-calendar"
    },
    "homepage": "https://github.com/synaiondigital/react-bali-calendar#readme",
    "bugs": {
        "url": "https://github.com/synaiondigital/react-bali-calendar/issues"
    }
}